{
    "name": "controlcode/cakephp-json-switch",
    "description": "Quick switch to json representation of any view variables.",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "json", "ldjson"],
    "homepage": "https://github.com/landr-controlcode/cakephp-json-switch",
    "license": "MIT",
    "authors": [
        {
            "name": "Landry Rakotoarivelo",
            "homepage": "https://www.control-code.com",
            "role": "Author"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ControlCodeJsonSwitch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ControlCodeJsonSwitch\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "minimum-stability": "stable"
}
